{
  "expo": {
    "name": "amongyall",
    "slug": "amongyall",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/amongyall_icon1.png",
    "scheme": "amongyall",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.lalam.amongyall",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app does not directly use the camera. The document picker feature may access the camera to scan documents."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/user-circle.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lalam.amongyall"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/user-circle.png"
    },
    "plugins": [
      "./plugins/withInfoPlist",
      "expo-apple-authentication",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/user-circle.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8062066976620321~5784637423",
          "iosAppId": "ca-app-pub-8062066976620321~5784637423"
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.lalam.amongyall",
          "enableGooglePay": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5ca35d0d-be67-4c80-9e99-71d166723c05"
      }
    }
  }
}